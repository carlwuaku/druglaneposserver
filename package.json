{
  "name": "DruglaneServer",
  "version": "1.0.0",
  "description": "Druglane POS local server",
  "productName": "DruglaneServer",
  "main": "main.js",
  "scripts": {
    "rebuild-sqlite": ".node_modules/.bin/electron-rebuild -w sqlite3 -p",
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager .  --overwrite --asar=true --platform=win32  --icon=app/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"DruglaneServer\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "deploy": "electron-builder build  --win --publish always"
  },
  "author": "Carl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlwuaku/druglaneposserver.git"
  },
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-builder": "^22.9.1",
    "electron-packager": "^14.2.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "electron-rebuild": "^1.11.0"
  },
  "dependencies": {
    "archiver": "^4.0.1",
    "bcryptjs": "^2.0.1",
    "body-parser": "^1.18.2",
    "decompress-zip": "^0.2.2",
    "ejs": "^3.1.3",
    "electron-log": "^4.2.1",
    "electron-updater": "^4.3.5",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.17.1",
    "formidable": "^1.2.2",
    "ip": "^1.1.5",
    "md5": "^2.2.1",
    "moment": "^2.27.0",
    "multiparty": "^4.2.2",
    "node-fetch": "^2.6.1",
    "node-os-utils": "^1.2.2",
    "node-schedule": "^1.3.2",
    "request": "^2.88.2",
    "sequelize": "^6.3.4",
    "socket.io": "^2.1.1",
    "sqlite-async": "^1.1.0",
    "sqlite3": "^5.0.0",
    "umzug": "^2.3.0",
    "xlsx": "^0.16.5"
  }
}
